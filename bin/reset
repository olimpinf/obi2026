#!/bin/sh                                                                                                    

psql postgres -c "DROP DATABASE obi2024"
psql postgres -c "DROP USER obi"
psql postgres -c "CREATE USER obi WITH PASSWORD 'guga.LC'"
psql postgres -c "CREATE DATABASE obi2024 OWNER obi ENCODING 'UTF8'"
