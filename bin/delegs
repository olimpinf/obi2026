#!/bin/bash

if [ -z "$2" ]; 
then 
    city=$1
else
    city="$1 $2"
fi
echo ${city}

psql -P pager=off -d obi2019 -U obi -c "select distinct school_id, school_name, school_deleg_name, school_deleg_phone, school_phone, school_deleg_email from school, compet where school_id=compet_school_id and compet_classif_fase2 and school_city ilike '%${city}%' and compet_type in (1,2,7) order by school_name;"
